,CAAA,OAAA;AAAA;AAFA,GAAA,CADA;AAMA,IAAA,UANA,wBAMA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA,QAAA,CAAA,UAAA,GAAA,KAAA,QAAA,CAAA;AACA,aAAA,KAAA,GAAA,CAAA,GAAA,KAAA,GAAA,CAAA;AACA;AATA,IAPA;AAkBA,EAAA,OAAA,oBACA,UAAA,CAAA,CAAA,aAAA,EAAA,YAAA,EAAA,eAAA,CAAA,CADA;AAGA,IAAA,UAHA,sBAGA,EAHA,EAGA;AACA,WAAA,OAAA,CAAA,IAAA,qBAAA,EAAA;AACA,KALA;AAMA,IAAA,mBANA,+BAMA,EANA,EAMA;AAAA;;AACA,UAAA,OAAA,CAAA,cAAA,CAAA,EAAA;AACA,aAAA,aAAA,CAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,EAAA;AACA,SAFA;AAGA;AACA,KAZA;AAaA,IAAA,QAbA,oBAaA,GAbA,EAaA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAfA;AAgBA,IAAA,MAhBA,oBAgBA;AACA,WAAA,WAAA,CAAA;AACA,QAAA,MAAA,EAAA,KAAA,IADA;AAEA,QAAA,QAAA,EAAA,KAAA;AAFA,OAAA;AAIA;AArBA,IAlBA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,SAAA,WAAA,CAAA;AACA,MAAA,MAAA,EAAA,KAAA,IADA;AAEA,MAAA,QAAA,EAAA,KAAA;AAFA,KAAA;AAIA;AA9CA,CAAA","sourcesContent":["<template>\n<div>\n  <v-btn color=\"primary\" @click=\"goRouter('/addContact')\">연락처 추가</v-btn>\n  <v-simple-table>\n    <template v-slot:default>\n      <thead>\n        <tr>\n          <th class=\"text-left\">사진</th>\n          <th class=\"text-left\">이름</th>\n          <th class=\"text-left\">전화번호</th>\n          <th class=\"text-left\">주소</th>\n          <th class=\"text-left\"></th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr class=\"contact\" @click=\"getContact(contact.no)\" v-for=\"( contact, index) in contacts.contacts\" :key=\"index\">\n          <td><img :src=\"contact.photo\" alt=\"\" width=\"50px\" height=\"50px\"></td>\n          <td>{{contact.name}}</td>\n          <td>{{contact.tel}}</td>\n          <td>{{contact.address}}</td>\n\n          <td>\n            <v-btn class=\"btn updateBtn\" color=\"primary\" @click.stop=\"goRouter(`/contacts/update/${contact.no}`)\">수정</v-btn>\n            <v-btn class=\"btn deleteBtn\" color=\"error\" @click.stop=\"onClickDeleteButton(contact.no)\">삭제</v-btn>\n          </td>\n        </tr>\n      </tbody>\n    </template>\n  </v-simple-table>\n  <v-container>\n    <v-row justify=\"center\" @click=\"goPage\">\n      <v-col cols=\"8\">\n        <v-container class=\"max-width\">\n          <v-pagination\n            v-model=\"page\"\n            class=\"my-4\"\n            :length=\"totalPages\"\n            :total-visible=\"9\"\n            circle\n          ></v-pagination>\n        </v-container>\n      </v-col>\n    </v-row>\n  </v-container>\n</div>\n</template>\n\n<script>\nimport { mapState, mapActions } from 'vuex'\n\nexport default {\n  data () {\n    return {\n      page: 1,\n      pagesize: 5\n    }\n  },\n  computed: {\n    ...mapState({\n      contacts: (state) => state.contacts.contacts,\n      contactDetail: (state) => state.contacts.contact\n    }),\n\n    totalPages () {\n      const total = Math.ceil(this.contacts.totalcount / this.pagesize)\n      return total > 0 ? total : 0\n    }\n  },\n  methods: {\n    ...mapActions(['getContacts', 'getContact', 'deleteContact']),\n\n    getContact (no) {\n      this.$router.push(`/contacts/${no}`)\n    },\n    onClickDeleteButton (no) {\n      if (confirm('정말 삭제하시겠습니까?')) {\n        this.deleteContact(no).then((res) => {\n          this.$router.go()\n        })\n      }\n    },\n    goRouter (url) {\n      this.$router.push(url)\n    },\n    goPage () {\n      this.getContacts({\n        pageno: this.page,\n        pagesize: this.pagesize\n      })\n    }\n  },\n  created () {\n    this.getContacts({\n      pageno: this.page,\n      pagesize: this.pagesize\n    })\n  }\n}\n</script>\n\n<style scoped>\n  .contact:hover {\n    cursor: pointer;\n  }\n\n  .v-list-item {\n    text-align: center;\n    border : 1px #e1e1e1 solid;\n    margin-bottom : 1px;\n  }\n</style>\n"],"sourceRoot":"src/js/components/views"}]}