n      console.log(no)\n    },\n    goRouter (url) {\n      this.$router.push(url)\n    }\n  },\n  created () {\n    this.getContacts()\n  }\n}\n</script>\n\n<style scoped>\n  .contact:hover {\n    cursor: pointer;\n  }\n\n  .v-list-item {\n    text-align: center;\n    border : 1px #e1e1e1 solid;\n    margin-bottom : 1px;\n  }\n</style>\n"],"sourceRoot":"src/js/components/views"}]}